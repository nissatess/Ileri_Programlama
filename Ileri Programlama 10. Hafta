
liste = []

elemansayisi = int ( giriş ( "Eleman vermeye girin" ))

toplam = 0
 ( 0 , elemansayısı ) aralığındaki i  için : 
    eleman = int ( giriş ( " eleman girin" ))
    listele _ ekleme ( eleman )
    toplam = toplam + eleman
    
    
yazdır ( "Liste = " , listele )
print ( "Elemanların toplamı =" , toplam )


dogruSayisi = int ( input ( "Doğru yemini girin" ))
yanlisSayisi = int ( input ( "Yanlış yemini girin" ))

dogruPuan = 0
yanlisPuan = 0
toplamPuan = 0

toplamSoru = doğruSayisi + yanlisSayisi

 toplamSoru == 10 ise :
    dogruPuan = dogruSayisi * 10
    yanlisPuan = yanlisSayisi * - 5
    toplamPuan = yanlisPuan + dogruPuan
    print ( "Toplam puaniniz =" , toplamPuan , "tir." )

başka:

    
    print ( "Soru sayılarını kontrol edin")
    
    
    
    
    
